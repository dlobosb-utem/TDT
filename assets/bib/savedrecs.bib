Editor = {Donati, ER and Viera, MR and Tavani, EL and Giaveno, MA and Lavalle, TL and Chiacchiarini, PA},
Title = {Protein function in extremely acidic conditions: molecular simulations
   of a predicted aquaporin and a potassium channel in Acidithiobacillus
   ferrooxidans},
Booktitle = {BIOHYDROMETALLURGY: A MEETING POINT BETWEEN MICROBIAL ECOLOGY, METAL
   RECOVERY PROCESSES AND ENVIRONMENTAL REMEDIATION},
